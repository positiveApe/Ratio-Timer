{
  "manifest_version": 2,
  "name": "Rest/Work Ratio Timer App",
  "version": "0.1",
  "content_scripts": [
  	{
    	"matches": [
      	"<all_urls>"
    	],
    	"js": ["content.js"]
  	}
],
"browser_action": {
	"default_popup": "popup.html",
	"default_icon": "icon-image.png",
	"default_title": "Have a good day" 
},
"chrome_url_overrides" : {  

},
"permissions": ["activeTab"]
}